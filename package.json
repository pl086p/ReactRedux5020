    "eslint": "^2.12.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.2",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "ncp": "^2.0.0",
    "postcss-loader": "^0.9.1",
    "promise": "^7.1.1",
    "react": "^15.1.0",
    "react-bootstrap": "^0.31.5",
